{
    "applicationId": "Dash Core UI",
    "version": "0.1.0",
    "defaultMode": "chrome",
    "port": 0,
    "documentRoot": "/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "tokenSecurity": "none",
    "logging": {
        "enabled": false,
        "writeToLogFile": false
    },
    "modes": {
        "chrome": {
            "width": 600,
            "height": 800,
            "minWidth": 400,
            "minHeight": 400,
            "args": "--user-agent=\"Neutralinojs chrome mode\" --disable-web-security"
        },
        "window": {
            "title": "Dash Core UI",
            "width": 600,
            "height": 800,
            "minWidth": 400,
            "minHeight": 400,
            "center": true,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/favicon.ico",
            "enableInspector": true,
            "borderless": false,
            "maximize": false,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": false
        }
    },
    "globalVariables": {
        "DASHD_URL": "http://127.0.0.1:9998/",
        "RPCUSER": "",
        "RPCPASSWORD": "",
        "WIN_DASHD": "../dashd.exe",
        "WIN_DASHD_PARAMS": "-blocksdir=d:\\\\",
        "LIN_DASHD": "../dashd",
        "LIN_DASHD_PARAMS": "",
        "MAC_DASHD": "../dashd",
        "MAC_DASHD_PARAMS": ""
    }
}